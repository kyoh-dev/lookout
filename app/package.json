{
	"name": "app",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@material/elevation": "^13.0.0",
		"@material/theme": "^13.0.0",
		"@smui/button": "^6.0.0-beta.15",
		"@smui/icon-button": "^6.0.0-beta.15",
		"@smui/top-app-bar": "^6.0.0-beta.15",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/mapbox-gl": "^2.6.4",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"dotenv": "^16.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.50.0",
		"smui-theme": "^6.0.0-beta.15",
		"svelte": "^3.44.0",
		"svelte-check": "^2.5.0",
		"svelte-preprocess": "^4.10.5",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"mapbox-gl": "^2.8.0"
	}
}
